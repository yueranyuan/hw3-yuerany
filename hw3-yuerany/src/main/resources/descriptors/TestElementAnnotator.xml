<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>  <annotatorImplementationName>edu.cmu.deiis.analysis.TestElementAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>TestElementAnnotator</name>
    <description>Parse out the test element components (question and answer) from the raw document text</description>
    <version>1.0</version>
    <vendor>yuerany 11791 project</vendor>
    <configurationParameters/>
    <configurationParameterSettings/>
    <typeSystemDescription>
      <imports>
        <import name="descriptors.deiis_types"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">edu.cmu.deiis.types.Question</type>
          <type>edu.cmu.deiis.types.Answer</type>
          <feature>edu.cmu.deiis.types.Answer:isCorrect</feature>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
  <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
